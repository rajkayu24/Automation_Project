<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SanitySuite">
<!--batch testing -executing multiple test case as a group called batch testing-->
<!--so here under sanity test suite we r executing multiple testclasses this is batch testing  -->
  <test thread-count="5" name="Test">
    <classes>
      <class name="testngBatchTesting.LoginTest"/>
      <class name="testngBatchTesting.SearchTest"/>
      <class name="testngBatchTesting.LogoutTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
